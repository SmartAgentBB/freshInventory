{
  "expo": {
    "name": "냉프로",
    "slug": "nengpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#26A69A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartagent.nengpro",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#26A69A"
      },
      "package": "com.smartagent.nengpro",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#26A69A",
      "androidMode": "default",
      "androidCollapsedTitle": "냉프로 알림"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.smartagent.nengpro",
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "44b88037-0b73-4cec-8c05-f0c6a57e823a"
      }
    }
  }
}
