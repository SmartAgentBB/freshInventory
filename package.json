{
  "name": "ez2cook",
  "version": "1.0.0",
  "description": "냉프로 - 당신의 주방을 스마트하게",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "validate-env": "node scripts/validate-env.js",
    "prebuild": "npm run validate-env",
    "bump:ios": "node scripts/bump-version.js ios",
    "bump:android": "node scripts/bump-version.js android",
    "bump:version": "node scripts/bump-version.js version",
    "build:ios": "node scripts/build-and-submit.js ios",
    "build:android": "node scripts/build-and-submit.js android",
    "build:status": "node scripts/build-and-submit.js check"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@supabase/supabase-js": "^2.45.4",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^4.1.0",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-font": "~14.0.8",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-notifications": "^0.32.11",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.1.3",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "expo-linking": "~8.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/freshInventory/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/**/index.ts"
    ]
  },
  "private": true
}
